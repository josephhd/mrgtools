%Matlab script
%Calculate characteristic impedance from width, thickness, and relative permittivity

%%%%%%%%%%%%%%%%%%%%%
% User input parameters
%%%%%%%%%%%%%%%%%%%%%%%%%%%
w1 = 2.3e-3; %strip width
d = 0.76 * 10^-3; %substrate thickness
er = 2.2; %relative permittivity


e_eff = ((er+1)/2) + ((er-1)/2)*(1/(sqrt(1+12*d/w1)));

ratio = w1/d;

if ratio <=1
    Z0 = (60/sqrt(e_eff))*log((8*d/w1) + (w1/(4*d)))
else 
    Z0 = 120*pi/(sqrt(e_eff)*(ratio + 1.393 + 0.667*log(ratio + 1.444)))
end

